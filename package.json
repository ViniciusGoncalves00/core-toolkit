{
  "name": "core-toolkit",
  "version": "0.1.3",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,json,md}\"",
    "fix": "npm run format && npm run lint",
    "clean": "rimraf dist"
  },
  "keywords": [
    "utils",
    "toolkit",
    "typescript"
  ],
  "author": "Vinicius Gon√ßalves <viniciusgoncalves2000@hotmail.com>",
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "eslint": "^9.39.1",
    "prettier": "^3.7.2"
  }
}
